{
  "name": "OpenAI GPT-2 - QLoRA-fp4 quantized",
  "transformerType": "OPENAI_GPT_2",
  "repo": "https://huggingface.co/PrunaAI/openai-community-gpt2-bnb-4bit-smashed",
  "files": ["config.json", "model.safetensors"],
  "parameterNaming": "transformer.{name}",
  "decoderParameterNaming": "transformer.h.{decoderId}.{name}",
  "memorySize": 7168,
  "quantization":
  {
    "quantizationType": "QLoRA",
    "outputFloatType": "FLOAT_16",
    "transposeMatrix": true
  }
}

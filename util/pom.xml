<?xml version="1.0" encoding="UTF-8"?>
<project>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>open-all-gpt-util</artifactId>
    <parent>
        <groupId>huplay</groupId>
        <artifactId>open-all-gpt-bom</artifactId>
        <version>1.0</version>
        <relativePath>../build</relativePath>
    </parent>

    <packaging>pom</packaging>

    <profiles>
        <profile>
            <id>standard</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <modules>
                <module>common</module>
                <module>standard</module>
            </modules>
        </profile>
        <profile>
            <id>nd4j</id>
            <modules>
                <module>common</module>
                <module>nd4j</module>
            </modules>
        </profile>
        <profile>
            <id>vector-api</id>
            <modules>
                <module>common</module>
                <module>vectorAPI</module>
            </modules>
        </profile>
    </profiles>

</project>